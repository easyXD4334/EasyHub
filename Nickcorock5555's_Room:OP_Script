-- Scripts:

local function LGVTG_fake_script() -- EasyRoom.LocalScript 
	local script = Instance.new('LocalScript', EasyRoom)

	wait()
	local GUI = script.Parent
	local Main = GUI.Frame
	
	local function SendMessage(Head,Message,Color1,Color2,Time)
		spawn(function()
			local M = GUI.Simple:Clone()
			M.Parent = GUI.Message
			M.Visible = true
			M.Frame.Head.Text = Head
			M.Frame.Message.Text = Message
			M.Frame.Head.TextColor3 = Color1
			M.Frame.Message.TextColor3 = Color2
	
			M.Frame:TweenPosition(
				UDim2.new(0,0,0,0),
				Enum.EasingDirection.Out,Enum.EasingStyle.Back,1
			)
			wait(Time)
			M.Frame:TweenPosition(
				UDim2.new(1,0,0,0),
				Enum.EasingDirection.Out,Enum.EasingStyle.Back,1
			)
			wait(1)
			M:Destroy()
		end)
	end
	
	local function PlaySound(ID)
		local Sound = Instance.new("Sound",script.Parent)
		Sound.SoundId = "rbxassetid://"..ID
		Sound.Volume = 5
		Sound:Play()
		
		Sound.Ended:Connect(function()
			Sound:Destroy()
		end)
	end
	
	--//Start Up\\--
	
	GUI.Frame.Visible = false
	script.Parent.Enabled = true
	GUI.TextLabel.Size = UDim2.new(0,0,0,0)
	GUI.TextLabel.Position = UDim2.new(0.482, 0,0.482, 0)
	SendMessage(
		"EasyHub",
		"EasyHub Loadded!",
		Color3.new(1,1,1),
		Color3.new(1,1,1),
		3
	)
	wait(1)
	PlaySound(8503531171)
	GUI.TextLabel:TweenSizeAndPosition(
		UDim2.new(0.064, 0,0.111, 0),UDim2.new(0.482, 0,0.482, 0),
		Enum.EasingDirection.Out,Enum.EasingStyle.Quad,1
	)
	game.TweenService:Create(
		GUI.TextLabel,TweenInfo.new(1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
		{Rotation = 360}
	):Play()
	wait(1)
	GUI.TextLabel:TweenPosition(
		UDim2.new(0.305, 0,0.332, 0),
		Enum.EasingDirection.Out,Enum.EasingStyle.Quad,1
	)
	game.TweenService:Create(
		GUI.TextLabel,TweenInfo.new(1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
		{Rotation = 0}
	):Play()
	wait(1)
	game.TweenService:Create(
		GUI.TextLabel.TextLabel,TweenInfo.new(0.5),{TextTransparency = 1}
	):Play()
	wait(0.5)
	GUI.TextLabel:TweenSize(
		UDim2.new(0.542, 0,0.066, 0),
		Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.5
	)
	wait(0.5)
	GUI.Frame.Size = UDim2.new(0.542, 0,0.066, 0)
	GUI.Frame.Visible = true
	GUI.Frame:TweenSize(
		UDim2.new(0.542, 0,0.571, 0),
		Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.5
	)
	wait(0.5)
	game.TweenService:Create(
		GUI.TextLabel,TweenInfo.new(0.5),{Transparency = 1}
	):Play()
	wait(.5)
	GUI.TextLabel:Destroy()
	
	
	
	--//Done Loadded\\--
	for loop,UI in pairs(GUI.Frame.Main:GetChildren()) do
		if UI:IsA("Frame") then
			UI.Main.MouseButton1Click:Connect(function()
				for loop,UI2 in pairs(GUI.Frame:GetChildren()) do
					if UI2:IsA("ScrollingFrame") and UI2.Name ~= "Main"  then
						if UI2.Name == UI.Main.Text then
							UI2.Visible = true
						else
							UI2.Visible = false
						end
					end
				end
			end)
		end
	end
	
	--//Hub Buttons\\--
	for loop,UI in pairs(GUI.Frame:GetChildren()) do
		if UI:IsA("ScrollingFrame") and UI.Name ~= "Main" then
			for loop,UI in pairs(UI:GetChildren()) do
				if UI:IsA("Frame") and UI:FindFirstChild("Frame") then
					--local UI = Main.Character.D
					local Bool = Instance.new("BoolValue",UI)
					UI.Frame.TextButton.MouseButton1Click:Connect(function()
						PlaySound(4499400560)
						if UI.Value.Value == false then
							UI.Value.Value = true
							game.TweenService:Create(
								UI.Frame.Frame,TweenInfo.new(0.5),
								{BackgroundColor3 = Color3.new(0,1,0)}
							):Play()
							UI.Frame.Frame:TweenPosition(
								UDim2.new(0.7, 0,0.092, 0),
								Enum.EasingDirection.Out,Enum.EasingStyle.Quad,
								0.5
							)
							
						else
							UI.Value.Value = false
							game.TweenService:Create(
								UI.Frame.Frame,TweenInfo.new(0.5),
								{BackgroundColor3 = Color3.new(1,0,0)}
							):Play()
							UI.Frame.Frame:TweenPosition(
								UDim2.new(0.055, 0,0.092, 0),
								Enum.EasingDirection.Out,Enum.EasingStyle.Quad,
								0.5
							)
						end
						
					end)
				end
			end
		end
	end
	
	--//Walk Speed\\--
	Main.Character.C.TextBox.FocusLost:Connect(function()
		local Input = tonumber(Main.Character.C.TextBox.Text)
		if Input then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Input
		else
			SendMessage(
				"Warning!","Enter number only!",
				Color3.new(1,0,0),Color3.new(1,1,1),3
			)
		end
	end)
	
	local Deo0 = false
	Main.Character.D.Value.Changed:Connect(function()
		if Main.Character.D.Value.Value == true then
			Deo0 = true
			while task.wait() do
				if Deo0 == true then
					local Input = tonumber(Main.Character.C.TextBox.Text)
					if Input then
						game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Input
					end
				else
					break
				end
			end
		else
			Deo0 = false
		end
	end)
	
	Main.Character.E.TextBox.FocusLost:Connect(function()
		local Input = tonumber(Main.Character.E.TextBox.Text)
		if Input then
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = Input
		else
			SendMessage(
				"Warning!","Enter number only!",
				Color3.new(1,0,0),Color3.new(1,1,1),3
			)
		end
	end)
	
	local Deo1 = false
	Main.Character.F.Value.Changed:Connect(function()
		if Main.Character.F.Value.Value == true then
			Deo1 = true
			while task.wait() do
				if Deo1 == true then
					local Input = tonumber(Main.Character.E.TextBox.Text)
					if Input then
						game.Players.LocalPlayer.Character.Humanoid.JumpPower = Input
					end
				else
					break
				end
			end
		else
			Deo1 = false
		end
	end)
	
	--//Visual\\--
	local Player = game.Players.LocalPlayer
	local Character = Player.Character
	local Humanoid =  Character:WaitForChild("Humanoid")
	local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
	
	Humanoid:UnequipTools()
	
	local FlashLight = Player.Backpack:FindFirstChild("Flashlight")
	
	Main.Visual._2.MouseButton1Click:Connect(function()
		Player.battery.life.Value = math.huge
	end)
	
	Main.Visual._3.MouseButton1Click:Connect(function()
		Player.battery.life.spare.Value = math.huge
	end)
	
	Main.Visual._4.MouseButton1Click:Connect(function()
		FlashLight.LightPart.SpotLight.Range = 60
		FlashLight.LightPart.SpotLight.Angle = 120
	end)
	
	local A60 = Instance.new("BillboardGui")
	local TextLabel = Instance.new("TextLabel")
	local A200 = Instance.new("BillboardGui")
	local TextLabel_2 = Instance.new("TextLabel")
	A60.Name = "A60"
	A60.Parent = game.StarterGui.EasyRoom.LocalScript
	A60.AlwaysOnTop = true
	A60.Size = UDim2.new(0, 200, 0, 50)
	TextLabel.Parent = A60
	TextLabel.BackgroundTransparency = 1
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.Size = UDim2.new(1, 0, 1, 0)
	TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
	TextLabel.TextScaled = true
	A200.Name = "A200"
	A200.AlwaysOnTop = true
	A200.MaxDistance = 500.000
	A200.Size = UDim2.new(0, 200, 0, 50)
	TextLabel_2.Parent = A200
	TextLabel_2.BackgroundTransparency = 1
	TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_2.Size = UDim2.new(1, 0, 1, 0)
	TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_2.TextScaled = true
	
	A60.Parent = workspace.monster
	A200.Parent = workspace.monster2
	
	workspace.monster.hunt.Changed:Connect(function()
		if workspace.monster.hunt.Value == true then
			SendMessage("Warning!","A-60 Spawn!",Color3.new(1,0,0),Color3.new(1,0,0),3)
		else
			SendMessage("A-60 Despawn","Safe for now...",Color3.new(1, 1, 1),Color3.new(1, 1, 1),3)
		end
	end)
	
	workspace.monster2.hunt.Changed:Connect(function()
		if workspace.monster2.hunt.Value == true then
			SendMessage("Warning!","A-200 Spawn!",Color3.new(1,0,0),Color3.new(1,0,0),3)
		else
			SendMessage("A-200 Despawn","Safe for now...",Color3.new(1, 1, 1),Color3.new(1, 1, 1),3)
		end
	end)
	
	
	
	Main.Visual._6.Value.Changed:Connect(function()
		if Main.Visual._6.Value.Value == true then
			workspace.CurrentCamera.CameraSubject = workspace.monster
		else
			workspace.CurrentCamera.CameraSubject = Humanoid
		end
	end)
	
	Player.CameraMode = Enum.CameraMode.Classic
	Player.CameraMaxZoomDistance = 10000
	
	while wait() do
		A60.TextLabel.Text = "A-60 | "..math.floor((HumanoidRootPart.Position - workspace.monster.Position).Magnitude/1.6).." M."
		A200.TextLabel.Text = "A-200 | "..math.floor((HumanoidRootPart.Position - workspace.monster2.Position).Magnitude/1.6).." M."
	end
end
coroutine.wrap(LGVTG_fake_script)()
local function ZGARPA_fake_script() -- Frame.DraggableGUI 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(ZGARPA_fake_script)()
